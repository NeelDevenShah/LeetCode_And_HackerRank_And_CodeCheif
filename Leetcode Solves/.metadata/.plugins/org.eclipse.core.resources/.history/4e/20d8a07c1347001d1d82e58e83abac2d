public class UniquePaths {
	public static void main(String [] args) {
		System.out.println(uniquePath(3, 2));
	}
	
	public static int uniquePath(int m, int n) {
		int arr[][]=new int[m][n];
		arr[m-1][n-1]=-1;
		arr[m-1][n-2]=1;
		arr[m-2][n-1]=1;
		
		for(int i=0;i<m-1;i++) {
			arr[i][n-1]=1;
		}
		for(int j=0;j<n-1;j++) {
			arr[m-1][j]=1;
		}
		
		for(int i=(m-2);i>=0;i--)
		{
			for(int j=(n-2);j>=0;j--){
				arr[i][j]=arr[i][j+1]+arr[i+1][j];
			}
		}
		return arr[0][0];
	}
}
